{
  "name": "bluedart-webhook-api",
  "version": "1.0.0",
  "description": "Blue Dart Push API Webhook - Receives and processes shipment tracking updates from Blue Dart",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "node test-webhook.js",
    "pm2:start:dev": "pm2 start ecosystem.config.js --only webhook-dev",
    "pm2:start:prod": "pm2 start ecosystem.config.js --only webhook",
    "pm2:stop:dev": "pm2 stop webhook-dev",
    "pm2:stop:prod": "pm2 stop webhook",
    "pm2:restart:dev": "pm2 restart webhook-dev",
    "pm2:restart:prod": "pm2 restart webhook",
    "pm2:delete:dev": "pm2 delete webhook-dev",
    "pm2:delete:prod": "pm2 delete webhook",
    "pm2:logs:dev": "pm2 logs webhook-dev",
    "pm2:logs:prod": "pm2 logs webhook",
    "pm2:monit": "pm2 monit",
    "pm2:status": "pm2 status",
    "migrate": "node database/migrate.js up",
    "migrate:down": "node database/migrate.js down",
    "migrate:list": "node database/migrate.js list"
  },
  "keywords": [
    "bluedart",
    "webhook",
    "api",
    "express",
    "mysql",
    "nodejs",
    "shipment-tracking"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "express": "^4.18.2",
    "mysql2": "^3.6.5",
    "dotenv": "^16.3.1",
    "cors": "^2.8.5",
    "body-parser": "^1.20.2"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "pm2": "^5.3.0"
  }
}
